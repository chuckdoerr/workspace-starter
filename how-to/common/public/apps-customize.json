		"publisher": "OpenFin",
		"intents": [
			{
				"name": "ViewContact",
				"displayName": "View Contact",
				"contexts": ["fdc3.contact"],
				"customConfig": {}
			}
		],
		"private": true,
		"customConfig": { "instanceMode": "single" },
		"images": [
			{
				"src": "http://localhost:8080/common/images/previews/view-contact-dashboard-launcher.png"
